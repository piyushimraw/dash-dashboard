{
  "name": "new-dash-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.28.1",
  "scripts": {
    "clean": "rm -rf apps/*/dist apps/*/dev-dist packages/*/dist packages/*/*.tsbuildinfo apps/*/node_modules packages/*/node_modules",
    "fresh": "pnpm clean && pnpm install && pnpm build",
    "dev": "pnpm --filter @apps/shell dev",
    "build": "pnpm --filter \"./packages/*\" build && pnpm --filter \"./apps/*\" build",
    "preview": "pnpm --filter @apps/shell preview",
    "preview:https": "./scripts/setup-https.sh && pnpm run build && pnpm run preview",
    "setup:https": "./scripts/setup-https.sh",
    "test": "vitest",
    "test:file": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . || true",
    "lint:fast": "oxlint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@hookform/resolvers": "^5.2.2",
    "@playwright/test": "^1.51.1",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.17",
    "@tanstack/react-query-persist-client": "^5.90.21",
    "@tanstack/react-router": "^1.146.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-plugin": "^1.147.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "babel-plugin-react-compiler": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "idb-keyval": "^6.2.2",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "lucide-react": "^0.562.0",
    "msw": "^2.12.7",
    "oxlint": "^1.43.0",
    "prettier": "^3.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "dependencies": {
    "@radix-ui/react-toast": "^1.2.15"
  }
}
